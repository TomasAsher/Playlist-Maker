<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ButtonActivityMain">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="backgroundTint">@color/yp_white</item>
        <item name="android:letterSpacing">0</item>
        <item name="textAllCaps">false</item>
        <item name="android:textColor">@color/yp_black</item>
        <item name="android:textSize">@dimen/_22sp</item>
        <item name="cornerRadius">@dimen/_16dp</item>
        <item name="iconGravity">textStart</item>
        <item name="iconTint">@color/yp_black</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>

    <style name="TextActivitySettings">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:minHeight">@dimen/_61dp</item>
        <item name="android:paddingStart">@dimen/_16dp</item>
        <item name="android:paddingEnd">@dimen/_12dp</item>
        <item name="android:textColor">@color/settingsTextColor</item>
        <item name="android:textSize">@dimen/_16sp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
    </style>
</resources>